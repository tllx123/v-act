{
  "compilerOptions": {
    "baseUrl": "./",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "incremental":true,
    "target": "ESNext",
    "paths": {
      "@v-act/*":["packages/*/src"]
    }
  },
  "include": ["packages/**/src"]
}